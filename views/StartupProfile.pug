doctype html
html(lang='en')
    head
        meta(charset='UTF-8', name='viewport', content='width = device-width initial-scale = 1')
        title=title
        link(rel='stylesheet', href='/out/semantic.css')
        // <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.3.1/components/icon.min.css">
        link(rel='stylesheet', href='stylesheets/style.css')
        style.
            .inputfile {
                width: 0.1px;
                height: 0.1px;
                opacity: 0;
                overflow: hidden;
                position: absolute;
                z-index: -1;
            }
            .card{
                box-shadow: none !important;
            }
    body
        #fixedmenu.ui.large.top.fixed.hidden.menu.secondary
            .ui.container
                a.item(href='#')
                    img.ui.small.image(src='images/logomoneygustX.png')
                a.active.item(href='Home.html') Invest
                a.item(href='AboutUs.html') About us
                a.item(href='howtoinvest.html') how to invest
                a.item(href='getFunded.html') Get Funding
                .right.menu
                    .ui.dropdown.item.login
                        .text
                            img.ui.avatar.image(src='https://semantic-ui.com/images/avatar2/large/kristy.png')
                            span#username
                                i.dropdown.icon
                        .menu
                            a.item(href='/StartupDashboard') Dashboard
                            a.item(href='/StartupDashboard') History
                            a.item(href='/StartupDashboard') Settings
                            a.item(href='/logout') Signed Out
        .ui.grid
            .four.wide.column
                .ui.special.cards
                    .card
                        .blurring.dimmable.image
                            .ui.dimmer
                                .content
                                    .center
                                        input#embedpollfileinput.inputfile(type='file', (change)='fileEvent($event)')
                                        label(for='embedpollfileinput')
                                            i.massive.link.camera.icon
                            img.ui.medium.rounded.image(src='https://semantic-ui.com/images/wireframe/square-image.png')
                        .content
